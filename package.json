{
  "name": "amazonq-sdd",
  "version": "0.1.0",
  "description": "Spec-driven development workflow for Amazon Q CLI users",
  "main": "dist/index.js",
  "bin": {
    "amazonq-sdd": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "amazon-q",
    "cli",
    "spec-driven-development",
    "sdd",
    "kiro",
    "development-workflow",
    "ai-assistant"
  ],
  "author": "Amazon Q SDD Contributors",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "handlebars": "^4.7.8",
    "fs-extra": "^11.2.0",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.6",
    "yaml": "^2.3.4",
    "glob": "^10.3.10"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^8.2.10",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/amazonq-sdd"
  },
  "bugs": {
    "url": "https://github.com/your-org/amazonq-sdd/issues"
  },
  "homepage": "https://github.com/your-org/amazonq-sdd#readme"
}